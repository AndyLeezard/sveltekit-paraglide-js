{
	"name": "@inlang/paraglide-js-adapter-sveltekit-example",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"test": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"postinstall": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.0.0-prerelease.20",
		"@inlang/paraglide-js-adapter-vite": "^1.0.3",
		"@inlang/plugin-message-format": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.20.4",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "4.5.0"
	},
	"type": "module"
}
